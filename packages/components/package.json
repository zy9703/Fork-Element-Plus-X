{
  "name": "vue-element-plus-x",
  "type": "module",
  "version": "1.1.4",
  "license": "MIT",
  "homepage": "https://element-plus-x.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeJiaYue520/Element-Plus-X.git"
  },
  "bugs": {
    "url": "https://github.com/HeJiaYue520/Element-Plus-X/issues",
    "email": "lijunsong2@gmail.com"
  },
  "keywords": [
    "vue",
    "element-plus",
    "element-plus-x",
    "vue-element-plus-x",
    "vue3",
    "vue3-element-plus",
    "vue3-element-plus-x",
    "vue3-element-plus-x-components"
  ],
  "main": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/umd/index.js",
  "types": "types/index.d.ts",
  "files": [
    "!dist/**/*.map",
    "README.md",
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build:es": "vue-tsc -b && vite build",
    "build:umd": "vue-tsc -b && vite build --config vite.config.umd.ts",
    "build": "rimraf dist && rimraf types && pnpm run aeac && pnpm run build:es && pnpm run build:umd",
    "preview": "vite preview",
    "ci:publish": "pnpm changeset publish -r",
    "aeac": "rimraf src/index.ts && rimraf src/install.ts && esno .build/scripts/auto-export-all-components.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "dompurify": "^3.2.4",
    "element-plus": "^2.9.7",
    "github-markdown-css": "^5.8.1",
    "markdown-it": "^14.1.0",
    "prismjs": "^1.30.0",
    "radash": "^12.1.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.3",
    "happy-dom": "^17.4.4",
    "rimraf": "^6.0.1",
    "sass": "^1.86.0",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-api-pro": "^0.0.1",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vite-plugin-prismjs": "^0.0.11",
    "vue-tsc": "^2.2.8"
  }
}
